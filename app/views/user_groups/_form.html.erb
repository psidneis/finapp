<%= simple_form_for(@user_group) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :enabled %>
    <%= f.input :role %>
    <%= f.association :group %>
    <%= f.association :user %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
